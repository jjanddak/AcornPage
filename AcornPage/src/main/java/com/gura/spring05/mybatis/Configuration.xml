<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 자주 사용하는 type 의 별칭 지정하기 -->
	<typeAliases>
		<typeAlias type="com.gura.spring05.member.dto.MemberDto" alias="memberDto"/>
		<typeAlias type="com.gura.spring05.users.dto.UsersDto" alias="usersDto"/>
	    <typeAlias type="com.gura.spring05.file.dto.FileDto" alias="fileDto"/>
	    <typeAlias type="com.gura.spring05.cafe.dto.CafeDto" alias="cafeDto"/>
	    <typeAlias type="com.gura.spring05.shop.dto.ShopDto" alias="shopDto"/>
	    <typeAlias type="com.gura.spring05.shop.dto.OrderDto" alias="orderDto"/>
	    <typeAlias type="com.gura.spring05.toondetail.dto.ToonDetailDto" alias="toonDetailDto"/>
	    <typeAlias type="com.gura.spring05.toonlist.dto.ToonListDto" alias="toonListDto"/>
	 	<typeAlias type="com.gura.spring05.star.dto.StarDto" alias="starDto"/>
	    <typeAlias type="com.gura.spring05.library.dto.LibraryDto" alias="libraryDto"/>
	    <typeAlias type="com.gura.spring05.toonup.dto.ToonupDto" alias="toonupDto"/>
	    <typeAlias type="com.gura.spring05.toon.dto.CommentLikeDto" alias="commentLikeDto"/>
	    <typeAlias type="com.gura.spring05.toon.dto.ToonCommentDto" alias="toonCommentDto"/>
	    <typeAlias type="com.gura.spring05.toonup.dto.NewToonupDto" alias="newToonupDto"/>
	</typeAliases>

	<!-- mapper 설정 -->
	<mappers>
		<mapper resource="com/gura/spring05/mybatis/MemberMapper.xml"/>
		<mapper resource="com/gura/spring05/mybatis/UsersMapper.xml"/>
		<mapper resource="com/gura/spring05/mybatis/FileMapper.xml"/>
		<mapper resource="com/gura/spring05/mybatis/CafeMapper.xml"/>
		<mapper resource="com/gura/spring05/mybatis/ToonCommentMapper.xml"/>
		<mapper resource="com/gura/spring05/mybatis/ShopMapper.xml"/>
		<mapper resource="com/gura/spring05/mybatis/ToonMapper.xml"/>
		<mapper resource="com/gura/spring05/mybatis/StarMapper.xml"/> 
	</mappers>
</configuration>




